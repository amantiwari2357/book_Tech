# Cloudinary Configuration
REACT_APP_CLOUDINARY_CLOUD_NAME=your-cloud-name
REACT_APP_CLOUDINARY_API_KEY=your-api-key
REACT_APP_CLOUDINARY_API_SECRET=your-api-secret
REACT_APP_CLOUDINARY_UPLOAD_PRESET=book-tech-preset

# Backend API URL
REACT_APP_API_URL=http://localhost:5000/api

# Other Configuration
REACT_APP_SITE_URL=https://www.book-tech.com
REACT_APP_SITE_NAME=Book-Tech
REACT_APP_SITE_DESCRIPTION=Your premier destination for digital books and reading experiences

# Payment Gateway (Razorpay)
REACT_APP_RAZORPAY_KEY_ID=your-razorpay-key-id
REACT_APP_RAZORPAY_KEY_SECRET=your-razorpay-key-secret

# Google Analytics
REACT_APP_GA_TRACKING_ID=your-ga-tracking-id

# Social Media
REACT_APP_FACEBOOK_APP_ID=your-facebook-app-id
REACT_APP_TWITTER_HANDLE=@booktech

# Email Configuration
REACT_APP_EMAIL_SERVICE=your-email-service
REACT_APP_EMAIL_TEMPLATE_ID=your-email-template-id

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_SOCIAL_LOGIN=true 